{
  "ConnectionStrings": {
    "Database": "Host=postgres;Port=5432;Database=Internship-Platform;Username=postgres;Password=postgres;Include Error Detail=true"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "ServerUrl": "http://seq:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "PaymobSettings": {
    "ApiKey": "",
    "SecretKey": "",
    "PublicKey": "",
    "HmacSecret": "",
    "BaseUrl": "https://accept.paymob.com/v1",
    "CardIntegrationId": 0,
    "WalletIntegrationId": 0
  },
  "JwtSettings": {
    "SigningKey": "",
    "Issuer": "Internship-Platform",
    "Audience": "SwaggerUI"
  },
  "Authentication": {
    "Google": {
      "ClientId": "",
      "ClientSecret": ""
    },
    "Github": {
      "ClientId": "",
      "ClientSecret": ""
    }
  },
  "SmtpSettings": {
    "Host": "smtp.gmail.com",
    "Port": 587,
    "Username": "",
    "Password": ""
  }
}